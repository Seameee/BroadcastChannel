{
  "functions": {
    "api/static/index.js": {
      "runtime": "edge"
    },
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ]
}
